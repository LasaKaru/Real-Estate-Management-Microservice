{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Yarp": "Information"
    }
  },
  "AllowedHosts": "*",
  "ReverseProxy": {
    "Routes": {
      "auth-route": {
        "ClusterId": "auth-cluster",
        "Match": {
          "Path": "/api/auth/{**catch-all}"
        }
      },
      "properties-route": {
        "ClusterId": "properties-cluster",
        "Match": {
          "Path": "/api/properties/{**catch-all}"
        }
      },
      "owners-route": {
        "ClusterId": "owners-cluster",
        "Match": {
          "Path": "/api/owners/{**catch-all}"
        }
      },
      "tenants-route": {
        "ClusterId": "tenants-cluster",
        "Match": {
          "Path": "/api/tenants/{**catch-all}"
        }
      },
      "leases-route": {
        "ClusterId": "leases-cluster",
        "Match": {
          "Path": "/api/leases/{**catch-all}"
        }
      },
      "payments-route": {
        "ClusterId": "payments-cluster",
        "Match": {
          "Path": "/api/payments/{**catch-all}"
        }
      },
      "maintenance-route": {
        "ClusterId": "maintenance-cluster",
        "Match": {
          "Path": "/api/maintenance/{**catch-all}"
        }
      },
      "auth-health": {
        "ClusterId": "auth-cluster",
        "Match": {
          "Path": "/api/auth-health"
        },
        "Transforms": [
          { "PathPattern": "/health" }
        ]
      },
      "properties-health": {
        "ClusterId": "properties-cluster",
        "Match": {
          "Path": "/api/properties-health"
        },
        "Transforms": [
          { "PathPattern": "/health" }
        ]
      },
      "owners-health": {
        "ClusterId": "owners-cluster",
        "Match": {
          "Path": "/api/owners-health"
        },
        "Transforms": [
          { "PathPattern": "/health" }
        ]
      },
      "tenants-health": {
        "ClusterId": "tenants-cluster",
        "Match": {
          "Path": "/api/tenants-health"
        },
        "Transforms": [
          { "PathPattern": "/health" }
        ]
      },
      "leases-health": {
        "ClusterId": "leases-cluster",
        "Match": {
          "Path": "/api/leases-health"
        },
        "Transforms": [
          { "PathPattern": "/health" }
        ]
      },
      "payments-health": {
        "ClusterId": "payments-cluster",
        "Match": {
          "Path": "/api/payments-health"
        },
        "Transforms": [
          { "PathPattern": "/health" }
        ]
      },
      "maintenance-health": {
        "ClusterId": "maintenance-cluster",
        "Match": {
          "Path": "/api/maintenance-health"
        },
        "Transforms": [
          { "PathPattern": "/health" }
        ]
      }
    },
    "Clusters": {
      "auth-cluster": {
        "Destinations": {
          "auth-service": {
            "Address": "http://localhost:5000"
          }
        }
      },
      "properties-cluster": {
        "Destinations": {
          "properties-service": {
            "Address": "http://localhost:5001"
          }
        }
      },
      "owners-cluster": {
        "Destinations": {
          "owners-service": {
            "Address": "http://localhost:5002"
          }
        }
      },
      "tenants-cluster": {
        "Destinations": {
          "tenants-service": {
            "Address": "http://localhost:5003"
          }
        }
      },
      "leases-cluster": {
        "Destinations": {
          "leases-service": {
            "Address": "http://localhost:5004"
          }
        }
      },
      "payments-cluster": {
        "Destinations": {
          "payments-service": {
            "Address": "http://localhost:5005"
          }
        }
      },
      "maintenance-cluster": {
        "Destinations": {
          "maintenance-service": {
            "Address": "http://localhost:5006"
          }
        }
      }
    }
  }
}
